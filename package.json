{
  "name": "cucumberx",
  "version": "1.0.0",
  "description": "cucumber",
  "main": "index.js",
  "scripts": {
    "results-junit:delete": "rm -rf cypress/results/junit/* || true",
    "results-junit:combine": "npx jrm cypress/results/junit/combined-report.xml \"cypress/results/junit/*.xml\"",
    "report:pre": "npm run results-junit:delete",
    "report:post": "npm run results-junit:combine",
    "hrm-test": "cypress run --spec 'cypress/e2e/Integration/Feature_files/Login.feature','cypress/e2e/Integration/Feature_files/Admin.feature'",
    "stanza-test": "cypress run --spec 'cypress/e2e/Integration/Feature_Files/Login_Page.feature','cypress/e2e/Integration/Feature_Files/Home_Page.feature' "
  },
  "author": "vipin",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.5",
    "cypress": "^14.5.2",
    "cypress-mochawesome-reporter": "^3.8.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.2.0",
    "multiple-cucumber-html-reporter": "^3.9.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "step_definitions": "cypress/e2e/Integration/Feature_Files"
  },
  "cucumberJson": {
    "generate": true,
    "outputFolder": "cypress/cucumber-json",
    "filePrefix": "",
    "fileSuffix": ".cucumber"
  },
  "dependencies": {
    "git-filter-repo": "^0.0.30"
  }
}
